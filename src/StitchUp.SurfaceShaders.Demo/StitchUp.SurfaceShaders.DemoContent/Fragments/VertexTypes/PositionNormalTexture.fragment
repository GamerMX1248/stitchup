fragment position_normal_texture;

[vertexattributes]
float3 position : POSITION;
float3 normal : NORMAL;

[interpolators]
float3 normal;

[vs]
__hlsl__
void main(INPUT input, inout OUTPUT output)
{
	export(float3, _Position, input.position);
	output(normal, input.normal);
}
__hlsl__

[ps]
__hlsl__
void main(INPUT input, inout OUTPUT output)
{
	export(float3, _WorldNormal, input.normal);
}
__hlsl__